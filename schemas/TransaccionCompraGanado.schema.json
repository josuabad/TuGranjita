  "$id": "transaccionCompraGanado",
  "title": "TransaccionCompraGanado",
  "type": "object",
  "properties": {
    "id_transaccion": { "type": "string" },
    "id_ganado": { "type": "string" },
    "origen": { "type": "string" },
    "pasta": { "type": "number", "minimum": 0 },
    "fecha_compra": { "type": "string", "format": "date" },
